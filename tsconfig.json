{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "lib": [
      "esnext"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "types": [
      "vitest/globals",
      "@cloudflare/workers-types"
    ],
    "strict": true,
    "declaration": true,
    "outDir": "dist",
    "esModuleInterop": true
  },
  "exclude": [
    "src/**/*.test.ts"
  ]
}
